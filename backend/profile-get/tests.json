{
  "tests": [
    {
      "name": "Get profile without token",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "success": false,
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Get profile with valid token",
      "method": "GET",
      "path": "/",
      "headers": {
        "X-Auth-Token": "test-token-placeholder"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "user": {
          "id": "number",
          "email": "string"
        }
      },
      "bodyMatcher": "partial"
    }
  ]
}
